package model;

import java.sql.Date;

public class Lembrete extends TarefaComData {
	private String hora;
	public Lembrete(String titulo, String descricao,Usuario usuario, Date data, String hora) {
		super(titulo, descricao, usuario, data);
		this.hora = hora;
	}
	
	public String getHora(){
		return hora;
	}
}
